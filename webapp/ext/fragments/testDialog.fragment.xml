<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:f="sap.ui.layout.form" xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1">
	<Dialog id="DetailsDialog" title="{i18n>testDialogTitle}">
		<content>
			<f:SimpleForm id="simpleFormChange" editable="true" class="editableForm sapUiSmallMarginTopBottom" width="100%">
				<f:content>
				</f:content>
			</f:SimpleForm>
		</content>
		<beginButton>
			<Button id="btnSave" text="{i18n>btnSave}" press="onSave" type="Accept"/>
		</beginButton>
		<endButton>
			<Button id="btnCancel" text="{i18n>btnCancel}" press="onCancel" type="Reject"/>
		</endButton>
	</Dialog>
</core:FragmentDefinition>